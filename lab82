var x = newArray();
x.push('a');
document.write( x.length);
x.push('b');
document.write( x[x.;ength-1] == 'b' );   document.write("<br>");
x.push('1');
document.write( x[x.;ength-1] == 1);    document.write("<br>");
x[x.length] = 'c';
document.write( x[x.length-1] );    document.write("<br>");

for (let i in x) document.write( x[i]"，" );    document.write("<br>");
for (let i =0;i < x.length;i++) document.write( x[i] + "，" );    document.write("<br>");
for (let y of x) document.write(y + "，");    document.write("<br>");
/* delete element ftom the tail:*/
let z = x.pop();
document.write("z=" + z + ", updated length=" + x.length);
let w = x.shift();
/* dequeue from head. enqueue at the tail*/
document.write("w=" + w + ",uppdated length=" + x.length);

x.push("x");
x.push("z");
document.write("x=" + x + "<br>");
x.splice(1,2);
document.write("x=" + x + "<br>");

x.splice(1,-1,'a','v','q','v');
document.write("x=" + x + "<br>");

document.write("x=" + x.join('~') + "<br>");
document.write("v is found at" + x.indexOf('v',3) + "<br>");

